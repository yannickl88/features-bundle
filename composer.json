{
    "name":              "yannickl88/features-bundle",
    "description":       "Symfony bundle for managing feature tags",
    "license":           "MIT",
    "type":              "symfony-bundle",
    "keywords": [
        "Symfony",
        "Features",
        "Bundle"
    ],
    "authors": [
        {
            "name":     "Yannick de Lange",
            "email":    "yannickl88@gmail.com",
            "homepage": "http://yannickl88.nl/"
        }
    ],
    "require": {
        "php":                          "^5.5|^7.0",
        "symfony/config":               "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/framework-bundle":     "^2.7|^3.0",
        "symfony/http-kernel":          "^2.7|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit":     "^4.8.35|^6.0",
        "symfony/finder":      "^2.7|^3.0",
        "symfony/twig-bundle": "^2.7|^3.0",
        "symfony/yaml":        "^2.7|^3.0",
        "twig/twig":           "^1.23.1|^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "autoload": {
        "psr-4": {
            "Yannickl88\\FeaturesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannickl88\\FeaturesBundle\\": "test/"
        }
    },
    "archive": {
        "exclude": [
            "/test"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
